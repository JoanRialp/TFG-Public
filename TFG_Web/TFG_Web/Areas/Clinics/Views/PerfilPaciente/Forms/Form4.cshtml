<div class="tab-pane fade" id="form4" role="tabpanel" aria-labelledby="navPagina4FormUsuario">
    <div class="tab-pane" id="home">
        <form asp-area="Clinics" asp-controller="PerfilPaciente" asp-action="form4PerfilPacientesGuardarInputs" method="post" asp-antiforgery="true">
            <input type="text" class="form-control" hidden="hidden" name="form4PerfilPacienteId" value="@Model.EnfermdedadPreexistente[0].Id">
            <input type="text" class="form-control" hidden="hidden" name="idPaciente" value="@Model.Pacientes.Id">
            <hr>
            <h3>Enfermdedad Preexistente</h3>
            <br />
            <div id="form4PerfilPacienteInputs">
                <div class="form-group">
                    <div class="col-xs-6">
                        <label for="first_name"><h5>Tipo de Enfermedad</h5></label>
                        <input type="text" class="form-control" disabled name="form4PerfilPacienteTipoEnfermedad" placeholder="Vacio" value="@Model.EnfermdedadPreexistente[0].A_Tipo">
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-6">
                        <label for="last_name"><h5>Edad de Diagnóstico</h5></label>
                        <input type="text" class="form-control" disabled name="form4PerfilPacienteEdadDiagnostico" placeholder="Vacio" value="@Model.EnfermdedadPreexistente[0].A_Edad">
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-6">
                        <label for="last_name"><h5>Otros (Internaciones, infecciones, etc.)</h5></label>
                        <input type="text" class="form-control" disabled name="form4PerfilPacienteOtros"  placeholder="Vacio" value="@Model.EnfermdedadPreexistente[0].A_Otros">
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-6">
                        <label for="last_name"><h5>Especificar</h5></label>
                        <input type="text" class="form-control" disabled name="form4PerfilPacienteEspecificar" placeholder="Vacio" value="@Model.EnfermdedadPreexistente[0].A_Especificar">
                    </div>
                </div>
            </div>
            <div id="buttonsSubmitForm4SistemaCompresorEditar" hidden="hidden" class="form-group">
                <div class="col-xs-12">
                    <br>
                    <button class="btn btn-lg btn-success" type="submit"><i class="glyphicon glyphicon-ok-sign"></i> Guardar</button>
                    <button id="buttonForm4SistemaCompresorEditarHidden" class="btn btn-lg btn-danger" type="button"><span class="fa fa-window-close"></span></button>
                </div>
            </div>
        </form>
    </div>
</div>