<div class="tab-pane fade" id="form2" role="tabpanel" aria-labelledby="navPagina2FormUsuario">
    <div class="tab-pane" id="home">
        <form asp-area="Clinics" asp-controller="PerfilPaciente" asp-action="form2PerfilPacientesGuardarInputs" method="post" asp-antiforgery="true">
            <input type="text" class="form-control" hidden="hidden" name="form2PerfilPacienteId" value="@Model.CirugiaPrevia[0].Id">
            <input type="text" class="form-control" hidden="hidden" name="idPaciente" value="@Model.Pacientes.Id">
            <hr>
            <h3>Cirugia Previa</h3>
            <br />
            <div id="form2PerfilPacienteInputs">
                <div class="form-group">
                    <div class="col-xs-6">
                        <label for="first_name"><h5>Tipo Cirugia</h5></label>
                        <input type="text" class="form-control" disabled name="form2PerfilPacienteTipoCirugia" id="" placeholder="Tipo Cirugia" value="@Model.CirugiaPrevia[0].CP_TipoCirugia">
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-6">
                        <label for="last_name"><h5>Edad</h5></label>
                        <input type="text" class="form-control" disabled name="form2PerfilPacienteEdad" id="" placeholder="Edad" value="@Model.CirugiaPrevia[0].CP_Edad">
                    </div>
                </div>
                <div id="buttonsSubmitForm2SistemaCompresorEditar" hidden="hidden" class="form-group">
                    <div class="col-xs-12">
                        <br>
                        <button class="btn btn-lg btn-success" type="submit"><i class="glyphicon glyphicon-ok-sign"></i> Guardar</button>
                        <button id="buttonForm2SistemaCompresorEditarHidden" class="btn btn-lg btn-danger" type="button"><span class="fa fa-window-close"></span></button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>